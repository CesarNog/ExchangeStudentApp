        <item name="android:paddingRight">0dp</item>
    </style>

    <style name="contact_list_item_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingRight">5dp</item>
    </style>

    <!-- Tela de Estatisticas -->
    <style name="list_of_carriers">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:background">@color/white</item>
        <item name="android:cacheColorHint">@color/white</item>
        <item name="android:drawSelectorOnTop">true</item>
        <item name="android:fastScrollEnabled">true</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_marginLeft">4dp</item>
        <item name="android:layout_marginRight">4dp</item>
        <item name="android:divider">@color/gray_light</item>
        <item name="android:dividerHeight">0.5px</item>
