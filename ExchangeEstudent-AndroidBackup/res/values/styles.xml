    <!-- Botão de voltar do Quem Somos -->
    <style name="about_us_button_back">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:layout_margin">3dp</item>
        <item name="android:text">@string/btn_back</item>
    </style>

    <!-- Dialog de ação na lista de contatos -->
    <!-- layout principal -->
    <style name="dialog_contact_action_main_layout" parent="@style/main_layout">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_vertical|center_horizontal</item>
        <item name="android:gravity">center_vertical|center_horizontal</item>
        <item name="android:padding">10dp</item>
    </style>

    <!-- Estilo correspondendo as linhas de elementos -->
    <style name="dialog_contact_action_line_layout">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <!-- Estilo da foto do contato -->
    <style name="dialog_contact_action_photo_layout">
        <item name="android:layout_width">110dp</item>
        <item name="android:layout_height">110dp</item>
        <item name="android:layout_gravity">right</item>
        <item name="android:padding">4dp</item>
    </style>

    <!-- Estilo da onde fica o nome e no número do contato -->
    <style name="dialog_contact_action_name_phone_layout">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
    </style>

    <!-- Estilo do nome do contato -->
    <style name="dialog_contact_action_name_layout" parent="@style/contact_list_item_style">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">18dp</item>
    </style>

    <!-- Estilo do telefone do contato -->
    <style name="dialog_contact_action_phone_layout" parent="@style/contact_list_item_style">
        <item name="android:textColor">@color/blue_light</item>
    </style>

    <!-- Estilo dos botões de acção -->
    <style name="dialog_contact_action_buttons_layout">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:layout_margin">2dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <!-- Estilo do botão encontrar operadora -->
    <style name="dialog_contact_action_update_carrier_layout" parent="@style/dialog_contact_action_buttons_layout">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:background">@drawable/btn_green</item>
    </style>

    <!-- Estilo de onde ficam os botões ligar e mensagem -->
    <style name="dialog_contact_action_buttons_call_message_layout">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <!-- Estilo do botão ligar -->
    <style name="dialog_contact_action_call_layout" parent="@style/dialog_contact_action_buttons_layout">
        <item name="android:background">@drawable/btn_blue</item>
    </style>

    <!-- Estilo do botão enviar mensagem -->
    <style name="dialog_contact_action_message_layout" parent="@style/dialog_contact_action_buttons_layout">
        <item name="android:background">@drawable/btn_blue</item>
    </style>

    <!-- Estilo do botão voltar -->
    <style name="dialog_contact_action_back_layout" parent="@style/about_us_button_back">
        <item name="android:layout_gravity">right</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <!-- Dialog animation -->
    <style name="PauseDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@null</item>
        <item name="android:windowAnimationStyle">@style/PauseDialogAnimation</item>
    </style>

    <style name="PauseDialogAnimation">
        <item name="android:windowEnterAnimation">@anim/fade_in</item>
        <item name="android:windowExitAnimation">@android:anim/fade_out</item>
    </style>

</resources>